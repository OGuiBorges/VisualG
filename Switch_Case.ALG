algoritmo "semnome"
var
   variable: inteiro
inicio
      escreva("Digite: ")
      leia(variable)
      escolha variable
      caso 1
         escreva("Churrasco1")
      caso 2
         escreva("Churrasco2")
      caso 3
         escreva("Churrasco3")
      caso 4
         escreva("Churrasco4")
      outrocaso
         escreva("Nada de Churrasco")
      fimescolha
fimalgoritmo